{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "google",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "space-before-function-paren": "off",
    "max-len": "off",
    "new-cap": "off"
  }
}
