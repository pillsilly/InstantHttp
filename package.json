{
  "name": "instantly_http",
  "version": "1.1.7",
  "description": "Tool to instantly create your own http server for development-use",
  "bin": "./dist/bin.js",
  "main": "./dist/run.js",
  "scripts": {
    "test": "npx jest --debug",
    "compile:prod": "rm dist/* && npx tsc --sourceMap false && ls -lha dist",
    "compile:esbuild:prod": "rm dist/* && npx esbuild src/bin.ts --bundle --platform=node --target=node18 --outdir=dist --packages=external && ls -lha dist",
    "release": "npx release-it",
    "build": "pkg . --targets=node14 --output instantHttp --compress=Brotli --no-bytecode  --public-packages=* --outputPath=dist"
  },
  "author": "pillsilly",
  "license": "MIT",
  "dependencies": {
    "chrome-launcher": "^0.13.4",
    "commander": "^9.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6"
  },
  "keywords": [
    "http",
    "server",
    "cli",
    "serve",
    "development",
    "tool",
    "http server"
  ],
  "homepage": "https://github.com/pillsilly/InstantHttp",
  "devDependencies": {
    "typescript": "^4.7.3",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^28.1.2",
    "esbuild": "^0.19.3",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^28",
    "pkg": "^5.8.1",
    "prettier": "^2.3.2",
    "release-it": "^15.6.0",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "useTabs": false,
    "trailingComma": "es5"
  }
}
