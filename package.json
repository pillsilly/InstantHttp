{
  "name": "instantly_http",
  "version": "1.1.11-0",
  "description": "Tool to instantly create your own http server for development-use",
  "bin": "./dist/bin.js",
  "main": "./src/index.ts",
  "scripts": {
    "test": "npx jest --debug",
    "compile:prod": "rm -f dist/* && npx tsc --sourceMap false && ls -lha dist",
    "compile:esbuild:prod": "rm dist/* -f && npx esbuild src/bin.ts  --bundle --minify --sourcemap --platform=node --target=node18 --outdir=dist   && ls -lha dist",
    "release": "npx release-it",
    "prebuild-binary": "npm run compile:esbuild:prod",
    "build-binary": "pkg ./dist/bin.js --targets=node18 --compress=Brotli -d --public-packages=* -o=dist/instant_http && ls -lha dist"
  },
  "author": "pillsilly",
  "license": "MIT",
  "dependencies": {
    "chrome-launcher": "^1.0.0",
    "commander": "^11.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6"
  },
  "keywords": [
    "http",
    "server",
    "cli",
    "serve",
    "development",
    "tool",
    "http server"
  ],
  "homepage": "https://github.com/pillsilly/InstantHttp",
  "devDependencies": {
    "typescript": "^4.7.3",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@types/jest": "^29.5.5",
    "esbuild": "^0.19.3",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29",
    "pkg": "^5.8.1",
    "prettier": "^3.0.3",
    "release-it": "^16.1.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "useTabs": false,
    "trailingComma": "es5"
  }
}
